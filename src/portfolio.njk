{% from "macros/highlighted.njk" import gbHighlighted %}

{% macro gbExperienceDateRange(props) %}
  {%- set range = (props.start or "") + " - " + (props.end or "") -%}
  {% if range | length == 3 %}
    {% set range = "" %}
  {% endif -%}
  {{ range }}
{% endmacro %}

{% macro gbExperienceTitle(props) %}
  <div class="flex justify-between border-b border-current">
    <h2 class="text-xl font-bold uppercase">{{ props.company }}</h2>
    <h2 class="text-xl font-bold">{{ gbExperienceDateRange(props) }}</h2>
  </div>
{% endmacro %}

{% macro gbExperiencePositionTitle(props) %}
  <div class="inline-block w-full font-bold">
    <h3 class="inline float-left {{ "border-b border-current" if props.underline }}">
      {{ props.title }}
      {%- if props.type -%}
        , <span class="italic font-normal">{{ props.type }}</span>
      {% endif %}
    </h3>
    <h3 class="inline float-right">{{ gbExperienceDateRange(props) }}</h3>
  </div>
{% endmacro %}

{% macro gbExperienceDescriptionItem(props) %}
  <li>
    {%- if props.text -%}
      {{ props.text }}
    {%- elif props.html -%}
      {{ props.html | safe }}
    {%- elif props.render -%}
      {{ props.render | render | safe }}
    {%- endif -%}
  </li>
{% endmacro %}

