<div class="mb-4">
  <p>The photos I took with the Asus ZenFone 5Z.</p>
</div>
<div class="flex flex-wrap -mx-1 overflow-hidden">
  {% for photo in photos %}
    {% set width = "md:w-1/2 lg:w-1/3 xl:w-1/3" %}
    {% set ratio = "aspect-ratio-1/1" %}
    {% if photo.width == 2 %}
      {% set width = "md:w-full lg:w-2/3 xl:w-2/3" %}
      {% set ratio = "aspect-ratio-2/1" %}
    {% elif photo.width == 3 %}
      {% set width = "md:w-full" %}
      {% set ratio = "aspect-ratio-3/1" %}
    {% endif %}

    <div class="my-1 px-1 w-full overflow-hidden {{ width }}">
      {% if photo.url %}
        <a target="_blank" href="{{ photo.url }}">
          <picture title="{{ photo.title }}" class="intrinsic bg-terniary dark:bg-secondary {{ ratio }}">
            <img src="{{ photo.src }}" alt="{{ photo.title }}" class="intrinsic-item object-cover border border-dotted border-accent"/>
          </picture>
        </a>
      {% else %}
        <picture title="{{ photo.title }}" class="intrinsic bg-terniary dark:bg-secondary {{ ratio }}">
          <img src="{{ photo.src }}" alt="{{ photo.title }}" class="intrinsic-item object-cover border border-dotted border-accent"/>
        </picture>
      {% endif %}
    </div>
  {% endfor %}
</div>